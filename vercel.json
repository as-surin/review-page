{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/dashboard.html"
    },
    {
      "src": "/dashboard",
      "dest": "/dashboard.html"
    },
    {
      "src": "/summary",
      "dest": "/summary.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
